SELECT


CONFIG. EVENT NAME, CONFIG. EVENT_ TYPE, DETAILS. STATUS, DETAILS. TRIGGERED AT , DETAILS. LAST IDENTIFIED _DATE,

(SELECT CP_NAME FROM XTF. CPHDR WHERE CP_CODE = CONFIG. CP CODE) AS CLient_ NAME, CONFIG MODULE_ TYPE ,
(SELECT EVENT NAME FROM
XTF. ALERT EVENTS WHERE

ALERT EVANT_ID - CONFIG. EVENT_RULE) EVENT_RULE, DETAILS. COMP MODULE_CODE,
DETAILS. COMP MODULE ID, DETAILS. COMP_ VALUE, CONFIG. TOLERANCE, PETAILS. ERROR DESC, CONFIG. RECURRENCE, CONFIG. DESCRIPTION, CONFIG. NEXT

DETAILS. ADD_ID, DETAILS. ADD_ DATE, DETAILS. UPDATE ID DETAILS, UEDATE _DATE
FROM
XTE. ALERT_CONFIG CONFIG, XTE. ALERT HEADER
DECATIS
WHERE


CONFIG. GUID = DETAILS. ALERT CONFIG GUID

AND ACTIVE=‘Y” AND (CONFIG. END DATE IS NULI OR CONFIG. END DATE 4= SYSDATE)

AND TRUNC (TRIGGERED AT) = TRUNC (SYSDATE) AND CLEARED BY IS NULI.

AND EVENT NAME- *00122BW*

ORDER BY TRUNC (TRIGGERED AT) desc;
_OCCURRENCE ,
